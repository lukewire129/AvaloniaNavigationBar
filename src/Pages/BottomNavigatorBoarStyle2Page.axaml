<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:style="clr-namespace:AvaloniaNavigationBar.Style"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaNavigationBar.Pages.BottomNavigatorBoarStyle2Page" Background="White"
             xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia">
    <style:BottomNavigatorBarStyle2>
        <ListBoxItem Tag="Home">
            <materialIcons:MaterialIcon Kind="Home"/>
        </ListBoxItem>
        <ListBoxItem Tag="Shop">
            <materialIcons:MaterialIcon Kind="Search"/>
        </ListBoxItem>
        <ListBoxItem Tag="Wishlist">
            <materialIcons:MaterialIcon Kind="HeartOutline"/>
        </ListBoxItem>
        <ListBoxItem Tag="Cart">
            <materialIcons:MaterialIcon Kind="Cart"/>
        </ListBoxItem>
        <ListBoxItem Tag="Me">
            <materialIcons:MaterialIcon Kind="PersonOutline"/>
        </ListBoxItem>
    </style:BottomNavigatorBarStyle2>
</UserControl>
